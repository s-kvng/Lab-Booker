<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.labbooker.labbooker.RepDashboard">
   <children>
      <VBox layoutX="38.0" layoutY="38.0" prefHeight="172.0" prefWidth="74.0" spacing="17.0">
         <children>
            <Label text="ID" />
            <Label text="Lab name" />
            <Label text="Date" />
            <Label text="Start Time" />
            <Label text="End Time" />
         </children>
      </VBox>
      <VBox layoutX="112.0" layoutY="38.0" prefHeight="172.0" prefWidth="174.0" spacing="8.0">
         <children>
            <TextField fx:id="tfID" />
            <ComboBox fx:id="labF" prefHeight="24.0" prefWidth="175.0" />
            <DatePicker fx:id="dateF" />
            <TextField fx:id="tfStartTime" layoutX="10.0" layoutY="58.0" />
            <TextField fx:id="tfEndTime" layoutX="10.0" layoutY="82.0" />
         </children>
      </VBox>
      <TableView fx:id="tvLabBookings" layoutX="293.0" layoutY="38.0" onMouseClicked="#handleMouseAction" prefHeight="441.0" prefWidth="503.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="43.0" text="ID" />
          <TableColumn fx:id="colLab" prefWidth="77.0" text="lab" />
            <TableColumn fx:id="colDate" prefWidth="108.0" text="Date" />
            <TableColumn fx:id="colClass" prefWidth="87.0" text="Class" />
            <TableColumn fx:id="colStartTime" prefWidth="90.0" text="startTime" />
            <TableColumn fx:id="colEndTime" prefWidth="97.0" text="EndTime" />
        </columns>
      </TableView>
      <Button fx:id="btnUpdate" layoutX="112.0" layoutY="247.0" mnemonicParsing="false" onAction="#handleBtnAction" text="Update" />
      <Button fx:id="btnDelete" layoutX="199.0" layoutY="247.0" mnemonicParsing="false" onAction="#handleBtnAction" text="Delete" />
   </children>
</AnchorPane>
